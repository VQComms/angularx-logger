{
    "name": "angularx-logger",
    "version": "2.0.0",
    "description": "Angularx-logger, a simple to use logger for all your logging needs!",
    "main": "./bundles/angularx-logger.umd.js",
    "module": "./esm5/angularx-logger.js",
    "es2015": "./esm2015/angularx-logger.js",
    "scripts": {
        "build": "node build.js",
        "test": "karma start",
        "pack:lib": "npm run build && npm pack ./dist",
        "publish:lib": "npm run build && npm publish ./dist",
        "publish:lib:next": "npm run build && npm publish --tag next ./dist",
        "compodoc": "compodoc -p tsconfig.json",
        "compodoc:serve": "compodoc -s"
    },
    "typings": "./angularx-logger.d.ts",
    "author": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/vqcomms/angularx-logger.git"
    },
    "bugs": {
        "url": "https://github.com/vqcomms/angularx-logger/issues"
    },
    "homepage": "https://github.com/vqcomms/angularx-logger",
    "keywords": [
        "angular",
        "angular2",
        "ng2",
        "ng2",
        "logger",
        "typescript",
        "log",
    "logging"
    ],
    "license": "MIT",
